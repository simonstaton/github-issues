{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "IS_CLIENT": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
     "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
     "no-underscore-dangle": 0,
     "import/prefer-default-export": 0,
     "consistent-return": 0
  }
}
